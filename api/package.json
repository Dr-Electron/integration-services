{
  "name": "e-commerce-audit-log",
  "version": "1.0.0",
  "description": "Audit log of the ensuresec project.",
  "main": "dist/index.js",
  "scripts": {
    "build-tsc": "tsc --project ./",
    "build-lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "create-dist-folder": "mkdir -p ./dist",
    "copy-streams-lib": "cp -R ./src/streams-lib/wasm-node/ ./dist/streams-lib/wasm-node/",
    "build-streams-lib": "npm-run-all create-dist-folder copy-streams-lib",
    "build": "npm-run-all build-tsc build-lint build-streams-lib",
    "test": "jest",
    "test-watch": "npm run test -- --watchAll",
    "start-node": "node dist/index.js",
    "start": "npm-run-all build start-node",
    "build-watch": "tsc --watch ",
    "serve-nodemon": "nodemon ./dist/index",
    "serve": "run-p build-watch serve-nodemon build-streams-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iotaledger/e-commerce-audit-log.git"
  },
  "keywords": [
    "ensuresec",
    "audit-log",
    "e-commerce",
    "iota",
    "iota-streams",
    "iota-identity"
  ],
  "author": "Dominic Zettl (IOTA Foundation)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/iotaledger/e-commerce-audit-log/issues"
  },
  "homepage": "https://github.com/iotaledger/e-commerce-audit-log#readme",
  "dependencies": {
    "@iota/identity-wasm": "0.3.4",
    "@sinclair/typebox": "^0.12.8",
    "ajv": "^8.3.0",
    "ajv-formats": "^2.1.0",
    "bs58": "^4.0.1",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-json-validator-middleware": "^2.1.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.4",
    "morgan": "^1.10.0",
    "noble-ed25519": "^1.0.3",
    "node-fetch": "^2.6.1",
    "winston": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
